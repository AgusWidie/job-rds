<script>
    var win = navigator.platform.indexOf('Win') > -1;
    if (win && document.querySelector('#Navbar-scrollbar')) {
        console.log('masuk ke if');
        var options = {
            damping: '0.5'
        }
        Scrollbar.init(document.querySelector('#Navbar-scrollbar'), options);
    }

    var myInterval = setInterval(TimeOutSession, 10000);
    function TimeOutSession() {
            console.log("CheckTimeOutSession");

            $.ajax({
                type: "POST",
                url: "@Url.Action("CheckSession", "Home")",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (data) {
                    if (data.bolSession === false) {
                        clearInterval(myInterval);
                        var actionUrl = '@Url.Action("Index", "Auth")';
                        window.location.href = actionUrl;
                    }

                },
                error: function (error) { },
            });


    }

</script>



